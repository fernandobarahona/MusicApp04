; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Music App 4"
run/main_scene="res://scenes_and_scripts/main.tscn"
config/icon="res://icon.png"

[autoload]

GlobalConstants="*res://scenes_and_scripts/gl_singl__global_constants/global_constants.gd"
ScreenManager="*res://scenes_and_scripts/gl_singl__screen_manager/screen_manager.gd"
AnimatedSceneManager="*res://scenes_and_scripts/gl_singl__animated_scene_manager/animated_scene_manager.gd"
ObjectAnimationsRepository="*res://scenes_and_scripts/gl_singl__object_animations_repository/object_animations_repository.gd"

[display]

window/size/width=720
window/size/height=1280
window/handheld/orientation="portrait"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
